{
  "compilerOptions": {
    "target": "ESNext",
  "module": "ESNext",
  "jsx": "react-jsx",
  "moduleResolution": "bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client"]
  }
}
